{
  "presets": [["env", { "modules": false }], "react"],
  "plugins": [
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "presets": ["env", "react"],
      "plugins": ["dynamic-import-node", "transform-class-properties"]
    }
  }
}
